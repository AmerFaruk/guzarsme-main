  <body>

    <div class="container">
      <h1>Calculate</h1>

      <div class="row">

        <div class="col-3">
          <button class="btn btn-danger" id="C">C</button>
        </div>
        <div class="col-6">
          <input type="text" id="txt">
        </div>
        <div class="col-3">
          <button class="btn btn-success" id="Cal"> CAL </button>
        </div>
        
        <div class="col-3 ">
